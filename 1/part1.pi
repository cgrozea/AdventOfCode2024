import util.
main=>
    L=read_file_lines("in.txt"),
    L1:=[],L2:=[],
    foreach(I in 1..L.length),
        C:=split(L[I]),
        L1:=[to_int(C[1])|L1],L2:=[to_int(C[2])|L2]
    end,
    L1:=sort(L1),L2:=sort(L2),
    S:=0,
    foreach(I in 1..L.length),
    S:=S+abs(L1[I]-L2[I])
    end,
    print(S).


